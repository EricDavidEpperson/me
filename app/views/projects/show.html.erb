<h1><%= @project.title %></h1>
<p><%= @project.about %></p>
<p><%= @project.technologies %></p>
<p><%= @project.screen_shot %></p>
<p><%= @project.url %></p>
<% if user_signed_in? %>
  <p><%= link_to "Edit", edit_project_path(@project) %> |
    <%= link_to "Delete", project_path(@project), method: :delete, "data-confirm"=>"Are you sure?" %></p>
<% end %>
